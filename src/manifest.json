{
    "manifest_version": 2,
    "name": "Angular Chrome Extension",
    "version": "1.0",
    "description": "A Chrome extension built with Angular",
    "browser_action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "activeTab"
    ],
    "web_accessible_resources": [
        "index.html"
    ]
}