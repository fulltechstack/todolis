{
    "manifest_version": 3,
    "name": "DoThis",
    "version": "1.0",
    "description": "Work-life balance made easy with DoThis for Chrome.",
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png"
        }
    },
    "permissions": [
        "activeTab",
        "storage"
    ]
}